{
  "parserOptions": {
    
    "sourceType": "module",
    "ecmaVersion": "latest",
    "requireConfigFile": false,
    "babelOptions": {
      "parserOpts": {
        "plugins": ["jsx"]
      }
    }
},
"parser": "@babel/eslint-parser",

  "extends": "plugin:@next/next/recommended",
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  }

}


